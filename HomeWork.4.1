// Реализовать класс для хранения комплексного числа. Выполнить в нем перегрузку всех необходимых
// операторов для успешной компиляции следующего
// фрагмента кода:
//          Complex z = new Complex(1,1);
//          Complex z1;
//          z1 = z - (z * z * z - 1) / (3 * z * z);
//          Console.WriteLine("z1={0}",z1);

using System;

namespace HomeWork2
{
    class Complex
    {
        double a;
        double b;

        public Complex(double a, double b)
        {
            this.a = a;
            this.b = b;
        }
        public override string ToString()
        {
            return $"{a}";
        }

        public static Complex operator +(Complex x, Complex y)
        {
            return new Complex(x.a + y.a, x.b + x.b);
        }

        public static Complex operator -(Complex x, Complex y)
        {
            return new Complex(x.a - y.a, x.b - x.b);
        }

        public static Complex operator *(Complex x, Complex y)
        {
            return new Complex(x.a * y.a - x.b * y.b, x.b * y.a + x.a * y.b);
        }

        public static Complex operator /(Complex x, Complex y)
        {
            return new Complex((x.a * y.a + x.b * y.b) / (y.a * y.a + y.b * y.b), (x.b * y.a - x.a * y.b) / (y.a * y.a + y.b * y.b));
        }
        public static Complex operator -(Complex x, int y)
        {
            return new Complex(x.a - y, x.b - y);
        }
        public static Complex operator *(int y, Complex x)
        {
            return new Complex(x.a * y, x.b * y);
        }
    }
    
    class Program
    {
        static void Main(string[] args)
        {
            Complex z = new Complex(1,1);
            Complex z1;
            z1 = z - (z * z * z - 1) / (3 * z * z);
            Console.WriteLine("z1={0}",z1);
        }
    }
}
